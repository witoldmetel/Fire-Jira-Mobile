{
  "name": "fire-jira-mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "pod": "pod install --project-directory=ios/",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx . --max-warnings 0",
    "lint:fix": "eslint --fix ./src",
    "type-check": "tsc",
    "test": "jest",
    "test:report": "jest --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --coverage",
    "ci": "yarn lint && yarn tsc",
    "clean": "yarn clean-deps && yarn clean-pods && yarn clean-gradle",
    "clean-deps": "rm -rf node_modules && yarn install",
    "clean-gradle": "cd android && ./gradlew clean cleanBuildCache",
    "clean-pods": "cd ios && pod deintegrate && pod install"
  },
  "dependencies": {
    "@react-native-firebase/app": "^17.4.2",
    "@react-native-firebase/auth": "^17.4.2",
    "@react-native-masked-view/masked-view": "^0.2.8",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/stack": "^6.3.4",
    "react": "18.1.0",
    "react-native": "0.70.6",
    "react-native-gesture-handler": "^2.8.0",
    "react-native-linear-gradient": "^2.7.3",
    "react-native-reanimated": "^2.14.4",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-native-svg": "^13.9.0",
    "react-native-vector-icons": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@ianvs/prettier-plugin-sort-imports": "^4.0.2",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^12.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.28.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.8",
    "prettier": "^2.7.1",
    "react-test-renderer": "18.1.0",
    "typescript": "4.8.3"
  }
}
